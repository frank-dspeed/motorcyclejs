{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "lib": [
      "dom",
      "es5",
      "es2015"
    ],
    "moduleResolution": "node",
    "module": "commonjs",
    "declaration": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "noUnusedParameters": true,
    "strictNullChecks": true
  },
  "include": [
    "src/**/*.ts"
  ]
}
